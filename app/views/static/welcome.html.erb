<h1 class="center">Welcome to Punchcard!</h1>

<div class="row">
  <div class="center container">
    <%= image_tag("/assets/logo.png", size: "400", class: "welcome-page") %>

    <div class="col s6">
      <%= button_to 'Sign In', new_user_session_path, class: 'btn-large', method: :get %>
    </div>

    <div class="col s6">
      <%= button_to 'Sign Up', new_user_registration_path, class: 'btn-large', method: :get %>
    </div>

  </div>
</div>
